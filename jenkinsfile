pipeline {
    agent any
    stages {
        stage('checkout'){
            steps {
            checkout([$class: 'GitSCM', branches: [[name: '*/python']], extensions: [], userRemoteConfigs: [[credentialsId: 'bcf85a0e-4f05-48b4-a228-1001879d3366', url: 'https://github.com/sindhug3/Demo-test.git']]])
                  }
        }
        stage('robot'){
            steps{    
            git branch: 'main', credentialsId: 'b7b48747-5465-4833-8753-f2cb77b57683', url: 'https://github.com/shailaja-j/First_Repo.git'
            }
        }
        
        stage('run test'){
            steps{
               sh'python serialtest.py'
               sh'python robot -m serialtest.robot'
            }
        }
  }
}
