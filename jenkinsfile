pipeline {
    agent any
 stages {
        stage('robot'){
            steps{  
                bat 'whoami'
                git branch: 'main', credentialsId: '82f0d861-8180-41a7-8ce7-cdecc9100607', url: 'https://github.com/shailaja-j/First_Repo.git'
            }
        }
        stage('run test'){
            steps{

                sh'python -m robot serialtest.robot'
                sh'python -m serialtest' 
            }
        }
  }
}